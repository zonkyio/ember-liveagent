{
  "name": "ember-liveagent",
  "version": "0.1.1",
  "description": "Salesforce's live agent chat integration for Ember.js applications.",
  "keywords": [
    "ember-addon",
    "salesforce",
    "liveagent",
    "chat"
  ],
  "homepage": "https://github.com/zonkyio/ember-liveagent#readme",
  "repository": "https://github.com/zonkyio/ember-liveagent.git",
  "bugs": {
    "url": "https://github.com/zonkyio/ember-liveagent/issues"
  },
  "license": "MIT",
  "author": "@zonkyio",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install",
    "release": "release-it",
    "changelog": "lerna-changelog"
  },
  "dependencies": {
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.6.1",
    "ember-cli": "~4.11.0",
    "ember-cli-code-coverage": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "~4.11.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.6.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-ember": "^11.4.8",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "husky": "^8.0.3",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^13.2.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.4",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.8.0",
    "webpack": "^5.76.2"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.1.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.md": "prettier --write"
  }
}
