{
  "name": "ember-liveagent",
  "version": "0.1.1",
  "description": "Salesforce's live agent chat integration for Ember.js applications.",
  "keywords": [
    "ember-addon",
    "salesforce",
    "liveagent",
    "chat"
  ],
  "homepage": "https://github.com/zonkyio/ember-liveagent#readme",
  "repository": "https://github.com/zonkyio/ember-liveagent.git",
  "bugs": {
    "url": "https://github.com/zonkyio/ember-liveagent/issues"
  },
  "license": "MIT",
  "author": "@zonkyio",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install",
    "release": "release-it",
    "changelog": "lerna-changelog"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.7",
    "ember-cli-htmlbars": "^6.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.4",
    "ember-cli": "~3.28.4",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.6",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.1.0",
    "husky": "^7.0.4",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^12.1.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.11.8",
    "release-it-lerna-changelog": "^4.0.1",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "14.17.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.md": "prettier --write"
  }
}
