{
  "name": "ember-liveagent",
  "version": "0.2.0",
  "description": "Salesforce's live agent chat integration for Ember.js applications.",
  "keywords": [
    "ember-addon",
    "salesforce",
    "liveagent",
    "chat"
  ],
  "homepage": "https://github.com/zonkyio/ember-liveagent#readme",
  "repository": "https://github.com/zonkyio/ember-liveagent.git",
  "bugs": {
    "url": "https://github.com/zonkyio/ember-liveagent/issues"
  },
  "license": "MIT",
  "author": "@zonkyio",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install",
    "release": "release-it",
    "changelog": "lerna-changelog"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@release-it-plugins/lerna-changelog": "^6.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.3.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-code-coverage": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.1.0",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.3.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.12.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "husky": "^8.0.3",
    "lerna-changelog": "^2.2.0",
    "lint-staged": "^14.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^3.0.0",
    "release-it": "^16.2.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "16.20.0",
    "npm": "8.19.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.md": "prettier --write"
  }
}
